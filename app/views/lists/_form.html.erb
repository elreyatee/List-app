<div>
  <%= form_for @list, html: {class: "form-inline"} do |f| %>
    <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name %>
    </div>
    <div class="form-group">
    <%= f.label :description %>
    <%= f.text_field :description %>
    </div>
    <%= f.submit 'Submit', class: "btn btn-primary" %>
  <% end %>
</div>
