<div>
  <%= link_to root_path do %>
    <i class="fa fa-home fa-2x"></i>
  <% end %> 
  <% if logged_in? %>
    <%= link_to "Logout", logout_path %> |
    <%= link_to "Create List", new_list_path %>
  <% else %>
    <%= link_to "Login", login_path %> |
    <%= link_to "Register", register_path %> |
  <% end %>
</div>
